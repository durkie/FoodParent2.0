  "dependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "exports-loader": "0.6.3",
    "imports-loader": "0.6.5",
    "jquery": "2.2.2",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "typescript": "1.8.0",
    "typings": "0.7.9",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "concurrently": "2.0.0",
    "webpack": "1.12.13"
  }
